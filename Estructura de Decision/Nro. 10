/*Hacer un programa para ingresar cinco números y listar el máximo de ellos.*/

#include<iostream>
using namespace std;
int main(void) {
 float n1,n2,n3,n4,n5;

    cout<<"Ingrese un numero: ";
    cin>>n1;
    cout<<"Ingrese un numero: ";
    cin>>n2;
    cout<<"Ingrese un numero: ";
    cin>>n3;
    cout<<"Ingrese un numero: ";
    cin>>n4;
    cout<<"Ingrese un numero: ";
    cin>>n5;

    if(n1>n2&&n1>n3&&n1>n4&&n1>n5){
        cout<<endl<<"El mayor es: "<<n1<<endl;
}
    else if(n2>n1&&n2>n3&&n2>n4&&n2>n5){
        cout<<endl<<"El mayor es: "<<n2<<endl;
    }
    else if(n3>n1&&n3>n2&&n3>n4&&n3>n5){
        cout<<endl<<"El mayor es: "<<n3<<endl;
    }
else if(n4>n1&&n4>n2&&n4>n3&&n4>n5){
        cout<<endl<<"El mayor es: "<<n4<<endl;
    }
    else if(n5>n1&&n5>n2&&n5>n3&&n5>n4){
        cout<<endl<<"El mayor es: "<<n5<<endl;
    }

	return 0;
}
